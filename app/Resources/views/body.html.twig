              <div style="display:none " id="TypeNoeudDiv">

                    <div>
                        <p style="margin-left: 20px">Les types des noeuds.</p>
                    </div>
                    <div>
                        <select style="margin-left: 20px" multiple id="TypeNoeud">
                            <option value="0">generic</option>
                            <option value="1">term</option>
                            <option value="2">form</option>
                            <option value="4">pos</option>
                            <option value="6">flpot</option>
                            <option value="8">chunk</option>
                            <option value="9">question</option>
                            <option value="10">relation</option>
                            <option value="12">analogy</option>
                            <option value="18">data</option>
                            <option value="36">data_pot</option>
                            <option value="444">link</option>
                            <option value="666">AKI</option>
                            <option value="777">wikipedia</option>
