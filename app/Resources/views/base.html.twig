    <script src="https://twitter.github.io/typeahead.js/releases/latest/typeahead.bundle.js"></script>
    <script src="{{ asset('js/typeahead.jquery.js') }}"></script>
    <script src="{{ asset('js/typeahead.bundle.js') }}"></script>
    <script>
        function myFunction1() {
            $("#RelinRelout").multiselect("show");
            var x = document.getElementById("RelinReloutDiv");
            x.style.display = "block";
            var y = document.getElementById("Filtrage");
            y.style.display = "block";

        }


        function myFunction2() {
            $("#RelEntrantes").multiselect({
                title: "Selectioner Les relations"
            });
            $("#RelEntrantes").multiselect("show");
            var x = document.getElementById("RelEntrantesDiv");
            x.style.display = "block";
            var y = document.getElementById("Filtrage");
            y.style.display = "block";

        }


        function myFunction3() {
            $("#RelSortantes").multiselect({
                title: "Selectioner Les relations"
            });
            $("#RelSortantes").multiselect("show");
            var x = document.getElementById("RelSortantesDiv");
            x.style.display = "block";
            var y = document.getElementById("Filtrage");
            y.style.display = "block";

        }

         function myFunction4() {
            $("#RelSortantes").multiselect({
                title: "Type de filtre"
            });
            $("#SortWeight").multiselect("show");
            var x = document.getElementById("SortWeightDiv");
            x.style.display = "block";
            var y = document.getElementById("Filtrage");
            y.style.display = "block";

        }

        var countries = new Bloodhound({
        datumTokenizer: Bloodhound.tokenizers.whitespace,
        queryTokenizer: Bloodhound.tokenizers.whitespace,
        // url points to a json file that contains an array of country names, see
        // https://github.com/twitter/typeahead.js/blob/gh-pages/data/countries.json
        prefetch: '{{ asset('mots.json') }}'
        });

        // passing in `null` for the `options` arguments will result in the default
        // options being used
        $('#dd .typeahead').typeahead(null, {
        name: 'mots',
        source: countries
        });
    </script>


